#cd db
mvn clean install -DskipTests

mvn install:install-file -Dfile=db/target/sonicbase-server-1.2.19.jar -DgroupId=com.sonicbase -DartifactId=sonicbase-server -Dversion=1.2.19 -Dpackaging=jar
mvn install:install-file -Dfile=jdbc/target/sonicbase-jdbc-1.2.19.jar -DgroupId=com.sonicbase -DartifactId=sonicbase-jdbc -Dversion=1.2.19 -Dpackaging=jar
mvn install:install-file -Dfile=jdbc/target/sonicbase-cli-1.2.19.jar -DgroupId=com.sonicbase -DartifactId=sonicbase-cli -Dversion=1.2.19 -Dpackaging=jar
