{
  "dataDirectory": "$HOME/db-data",
  "installDirectory": "$HOME/sonicbase",
  "copmressRecords": false,
  "useUnsafe": true,
  "maxJavaHeap": "20%",
  "maxJavaHeapTrigger": "95%",
  "user": "ubuntu",
  "maxProcessMemoryTrigger": "90%",
  "clientIsPrivate": false,
  "shards" : [
    {
      "replicas": [
