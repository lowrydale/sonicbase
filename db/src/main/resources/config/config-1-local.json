{
    "database": {
        "dataDirectory": "$HOME/db-data",
        "installDirectory": "$HOME/database",
        "partitionCountForRecordIndex" : 10000,
        "subPartitionsForIdIndex" : 10,
        "initialIndexSize" : 10000000,
        "indexEntrySize" : 60,
        "maxJavaHeap": "50%",
        "maxProcessMemory": "90%",
      "replicationFactor": 1,
        "shards" : [
          {
            "replicas": [
              {
                "privateAddress": "localhost",
                "publicAddress": "localhost",
                "port": 9010
              }
            ]
          }
        ]
    }

}
